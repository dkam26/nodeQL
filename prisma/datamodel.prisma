type User {
    id: ID! @id
    name: String!
    email: String! @unique
    password: String!
    shoppinglists: [Shoppinglist!]!
}

type Shoppinglist {
    id: ID! @id
    name: String!
    createdAt: DateTime! @createdAt
    items:[Item!]!
}

type Item {
    id: ID! @id
    name: String!
    price: Int!
}